<idea-plugin>
    <id>com.github.fornever.intellijdebuggersample</id>
    <name>Debugger Sample</name>
    <vendor>fornever</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.DebuggerBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <xdebugger.breakpointType implementation="me.fornever.autohotkey.debugger.AutoHotKeyBreakpointType"/>
        <configurationType implementation="me.fornever.autohotkey.debugger.AutoHotKeyRunConfigurationType"/>
        <runConfigurationProducer implementation="me.fornever.autohotkey.debugger.AutoHotKeyRunConfigurationProducer"/>
        <programRunner implementation="me.fornever.autohotkey.debugger.AutoHotKeyDebugProgramRunner"/>
    </extensions>
</idea-plugin>
